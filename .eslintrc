{	
  "rules": {	
    "max-len": 0,	
    "comma-dangle": 0,	
    "no-console": 0,	
    "padded-blocks": 0,	
    "indent": [	
      2,	
      2,	
      {	
        "SwitchCase": 1	
      }	
    ],	
    "spaced-comment": 1,	
    "quotes": [	
      "error",	
      "single",	
      {	
        "allowTemplateLiterals": true	
      }	
    ],	
    "import/prefer-default-export": "off",	
    "arrow-parens": 0,	
    "consistent-return": 0,	
    "no-useless-escape": 0,	
    "no-underscore-dangle": 0,	
    "no-undef": "error"	
  },	
  "extends": ["prettier"],	
  "plugins": ["prettier"],	
  "env": {	
    "browser": false,	
    "node": true,	
    "es6": true,
    "jest": true
  }	
}